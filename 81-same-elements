public Boolean sameElements(List<Integer> nums1, List<Integer> nums2) {
    List<Integer> numsAdd = new List<Integer>();
    numsAdd.addAll(nums1);
    numsAdd.addAll(nums2);

    Set<Integer> numsSet1 = new Set<Integer>(nums1);
    Set<Integer> numsSet2 = new Set<Integer>(nums2);
    Set<Integer> numsSetAdd = new Set<Integer>(numsAdd);

    if(numsSetAdd.size() == numsSet1.size() && numsSet1.size() == numsSet2.size()){
        return true;
    }
    else{
        return false;
    }
}