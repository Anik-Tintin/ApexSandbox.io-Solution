public boolean isAnagram(String s1, String s2) {
    List<String> sl1 = s1.split('');
    List<String> sl2 = s2.split('');

    sl1.sort();
    sl2.sort();

    if(sl1.size()!=sl2.size()){
        return false;
    }

    for(Integer i=0;i<sl1.size();i++){
        if(sl1[i] != sl2[i]){
            return false;
        }
    }
    return true;
}