public Set<Integer> setUnion(Set<Integer> set1, Set<Integer> set2) {
    //code here
    List<Integer> unionList = new List<Integer>();
    Set<Integer> unionSet = new Set<Integer>();
    unionList.addAll(set1);
    unionList.addAll(set2);
    unionSet = new Set<Integer>(unionList);
    return unionSet;
}