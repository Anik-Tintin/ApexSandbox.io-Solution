public List<String> orgNames(List<String> adjectives, List<String> animals) {
    List<String> orgNamesInst = new List<String>();
    String orgName = '';
    for(String adj : adjectives){
        for(String animal : animals){
            orgName = adj+'-'+animal;
            orgNamesInst.add(orgName);
        }
    }
    return orgNamesInst;
}